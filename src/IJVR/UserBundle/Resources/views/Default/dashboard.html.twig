{% extends "::layout.html.twig" %}

{% block title %}
  Dashboard - {{ parent() }}
{% endblock %}

{% block body %}

<div class="container">

<h2 style="">Dashboard</h2>

<div class="col-md-4">
<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading"><h4>Most used Keywords</h4></div>

  <!-- Table -->
  <table class="table">

  	{%for i in  1..20 %}
  		<tr><td><h4>{{i}}</h4></td><td>
		<h4>#{{keywords[i].keyword}} </h4>
		</td>
	{%endfor%}
    
  </table>
</div>
</div>


{% endblock %}